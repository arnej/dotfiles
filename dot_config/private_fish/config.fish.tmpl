if status is-interactive

    # Use vi key bindings
    fish_vi_key_bindings

    abbr -a cm 'chezmoi'
    abbr -a dotb 'dotnet build'
    abbr -a dotcb 'dotnet clean && dotnet build'
    abbr -a dotr 'dotnet restore --interactive'
    abbr -a dott 'dotnet test'
    abbr -a ga 'git add'
    abbr -a gbr 'git branch'
    abbr -a gc 'git commit -v'
    abbr -a gcl 'git clean -f'
    abbr -a gco 'git checkout'
    abbr -a gco. 'git checkout -- .'
    abbr -a gcp 'git cherry-pick'
    abbr -a gd 'git diff'
    abbr -a gdc 'git diff --cached'
    abbr -a gf 'git fetch'
    abbr -a gfp 'git fetch --prune'
    abbr -a gl 'git pull'
    abbr -a glg 'git log --stat'
    abbr -a gm 'git merge'
    abbr -a gma 'git merge --abort'
    abbr -a gmc 'git merge --continue'
    abbr -a gmt 'git mergetool'
    abbr -a gp 'git push'
    abbr -a grb 'git rebase'
    abbr -a grba 'git rebase --abort'
    abbr -a grbc 'git rebase --continue'
    abbr -a grs 'git reset'
    abbr -a gsh 'git show'
    abbr -a gsp 'git stash pop'
    abbr -a gss 'git stash show'
    abbr -a gst 'git status'
    abbr -a gsu 'git stash -u'
    abbr -a gwt 'git worktree'
    abbr -a gwta 'git worktree add'
    abbr -a gwtl 'git worktree list'
    abbr -a gwtr 'git worktree remove'
    abbr -a ip 'ip -c=auto'
    abbr -a todo 'nvim ~/syncthing-arne/todo/todo.txt'
    abbr -a vim 'nvim'

    fish_add_path -p ~/.local/bin ~/.cargo/bin

    # Init ssh-agent
    if not pgrep --full ssh-agent | string collect >/dev/null
        eval (ssh-agent -c)
        set -Ux SSH_AGENT_PID $SSH_AGENT_PID
        set -Ux SSH_AUTH_SOCK $SSH_AUTH_SOCK
    end

    # Activate mise to be able to use other stuff installed via mise
    mise activate fish | source

    # Initialize some stuff
    starship init fish | source
    zoxide init fish | source

    fastfetch
end
